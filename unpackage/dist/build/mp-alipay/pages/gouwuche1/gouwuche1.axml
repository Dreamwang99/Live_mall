<view style="background-color:#F5F5F5;"><view class="head"><view class="head_title"><view style="width:50%;font-size:35rpx;">购物车</view><view data-event-opts="{{[['tap',[['manage']]]]}}" onTap="__e">{{guanli}}</view></view></view><view class="goods-list"><block a:if="{{goodsList.length==0}}"><view class="tis">购物车是空的哦~</view></block><block a:for="{{goodsList}}" a:for-item="row" a:for-index="index" a:key="index"><view class="row"><view data-event-opts="{{[['tap',[['deleteGoods',['$0'],[[['goodsList','',index,'id']]]]]]]}}" class="menu" catchTap="__e"><view class="icon shanchu"></view></view><view data-event-opts="{{[['touchStart',[['touchStart',[index,'$event']]]],['touchMove',[['touchMove',[index,'$event']]]],['touchEnd',[['touchEnd',[index,'$event']]]]]}}" class="{{(('carrier')+' '+(theIndex==index?'open':oldIndex==index?'close':''))}}" onTouchStart="__e" onTouchMove="__e" onTouchEnd="__e"><view class="heng"><image class="dp" src="../../static/gouwuche/iocn-22-dp.png" mode></image><view class="dpzi">某某旗舰店</view></view><view data-event-opts="{{[['tap',[['selected',[index]]]]]}}" class="checkbox-box" onTap="__e"><view class="checkbox"><image class="on" src="{{row.selected?'../../static/center/iocn-gou.png':''}}" mode></image></view></view><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['goodsList','',index]]]]]]]}}" class="goods-info" onTap="__e"><view class="img"><image src="{{row.img}}"></image></view><view class="info"><view class="title">{{row.name}}</view><view class="spec">{{row.spec}}</view><view class="price-number"><view class="price">{{"￥"+row.price}}</view><view class="number"><view data-event-opts="{{[['tap',[['sub',[index]]]]]}}" class="sub" catchTap="__e"><view class="icon jian">-</view></view><view data-event-opts="{{[['tap',[['discard',['$event']]]]]}}" class="input" catchTap="__e"><input type="number" data-event-opts="{{[['input',[['__set_model',['$0','number','$event',[]],[[['goodsList','',index]]]],['sum',['$event',index]]]]]}}" value="{{row.number}}" onInput="__e"/></view><view data-event-opts="{{[['tap',[['add',[index]]]]]}}" class="add" catchTap="__e"><view class="icon jia">+</view></view></view></view></view></view></view></view></block></view><view class="footer" style="{{'bottom:'+(footerbottom)+';'}}"><view data-event-opts="{{[['tap',[['allSelect',['$event']]]]]}}" class="checkbox-box" onTap="__e"><view class="checkbox"><image class="on" src="{{isAllselected?'../../static/center/iocn-gou.png':''}}" mode></image></view><view class="text">全选</view></view><block a:if="{{!a}}"><view data-event-opts="{{[['tap',[['deleteList',['$event']]]]]}}" class="delBtn" onTap="__e">删除</view></block><block a:if="{{a}}"><view class="settlement"><view class="sum">合计:<view class="money">{{"￥"+sumPrice}}</view></view><view data-event-opts="{{[['tap',[['toConfirmation',['$event']]]]]}}" class="btn" onTap="__e">结算</view></view></block></view></view>