<view><view class="VerticalBox"><scroll-view class="VerticalNav nav" style="height:calc(100vh - 375rpx);" scroll-y scroll-with-animation scroll-top="{{verticalNavTop}}"><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><view class="{{(('cu-item')+' '+(index==tabCur?'text-green cur':''))}}" data-id="{{index}}" data-event-opts="{{[['tap',[['TabSelect',['$event']]]]]}}" onTap="__e">{{''+item.name+''}}</view></block></scroll-view><scroll-view class="VerticalMain" style="height:calc(100vh - 375rpx);" scroll-y scroll-with-animation scroll-into-view="{{'main-'+mainCur}}" data-event-opts="{{[['scroll',[['VerticalMain',['$event']]]]]}}" onScroll="__e"><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><view class="padding-top padding-lr" id="{{'main-'+index}}"></view></block><view class="cu-list menu-avatar"><view class="cu-item"><view class="meizhuang">美妆</view><view class="hzp1"><block a:for="{{list1}}" a:for-item="item" a:for-index="index" a:key="index"><view class="hezi"><image class="{{(item.tu)}}" src="{{item.urlimage}}" mode="aspectFit"></image><view class="hzp2">{{item.name}}</view></view></block></view></view></view></scroll-view></view></view>