.content,page{background-color:#f8f8f8;height:100%;overflow:hidden}.nav-bar{position:relative;z-index:10;height:90rpx;white-space:nowrap;-webkit-box-shadow:0 2rpx 8rpx rgba(0,0,0,.06);box-shadow:0 2rpx 8rpx rgba(0,0,0,.06);background-color:#fff}.nav-bar .nav-item{display:inline-block;width:122rpx;height:90rpx;text-align:center;line-height:90rpx;font-size:28rpx;color:#000;position:relative}.nav-bar .nav-item:after{content:"";width:0;height:0;border-bottom:4rpx solid #f23d3e;position:absolute;left:50%;bottom:0;-webkit-transform:translateX(-50%);-ms-transform:translateX(-50%);transform:translateX(-50%);-webkit-transition:.3s;-o-transition:.3s;transition:.3s}.nav-bar .current{color:#f23d3e}.nav-bar .current:after{width:50%}.swiper-box{height:100%}.panel-scroll-box{height:100%}.panel-scroll-box .panel-item{background:#fff;padding:30px 0;border-bottom:2px solid #000}.news-item{width:99%;height:100%;margin-top:20rpx}.news-item-content{width:95%;height:450rpx;margin-left:2.5%;border-radius:20rpx;background-color:#fff}.level{display:inline-block;display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex;float:left}.news-item-content-head{width:95%;height:80rpx;margin-left:2.5%;border-radius:20rpx;background-color:#fff;padding-top:30rpx}.news-item-content-content{width:95%;height:200rpx;margin-left:2.5%;border-radius:20rpx;background-color:#fff;padding-top:30rpx}.news-item-content-img{width:200rpx;height:200rpx}.news-item-content-bottom{width:90%;height:50rpx;margin-left:5%;border-radius:20rpx;background-color:#fff;margin-top:20rpx}.news-item-content-content-info{font-size:14px;-o-text-overflow:-o-ellipsis-lastline;text-overflow:-o-ellipsis-lastline;overflow:hidden;-o-text-overflow:ellipsis;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:2;line-clamp:2;-webkit-box-orient:vertical}.news-item-content-bottom-confirm{width:170rpx;height:50rpx;color:#fff;background-color:#f23d3e;float:right;border-radius:50rpx;margin-left:15rpx;text-align:center;border:1px solid #f23d3e}.news-item-content-bottom-cancel{width:170rpx;height:50rpx;color:#949494;background-color:#fff;float:right;border-radius:50rpx;border:1px solid #949494;text-align:center}