<view><view class="topTabBar" style="{{'position:'+(headerPosition)+';'+('top:'+(headerTop)+';')}}"><block wx:for="{{orderType}}" wx:for-item="grid" wx:for-index="tbIndex" wx:key="tbIndex"><view data-event-opts="{{[['tap',[['showType',[tbIndex]]]]]}}" class="grid" bindtap="__e"><view class="{{['text',tbIndex==tabbarIndex?'on':'']}}">{{grid}}</view></view></block></view><view class="order-list"><view class="list"><block wx:if="{{t.length==0}}"><view class="onorder"><view class="text">没有相关订单</view></view></block><block wx:for="{{t}}" wx:for-item="row" wx:for-index="index" wx:key="index"><block wx:if="{{t.length>0}}"><view class="row"><view class="type"><image class="level" src="../../static/dingdan/iocn-29-dp.png" mode></image>{{row.business_name+''}}</view><block wx:for="{{row.goods_list}}" wx:for-item="list" wx:for-index="idx" wx:key="idx"><view data-event-opts="{{[['tap',[['godetail']]]]}}" class="order-info" bindtap="__e"><view class="left"><image src="{{list.goods_logo}}"></image></view><view class="right"><view class="name">{{''+list.goods_title+''}}</view><view class="spec">{{list.goods_spec}}</view><view class="prices">{{'￥'+list.price_real+''}}</view></view><view class="number">{{"X"+list.number_goods}}</view></view></block><view class="btns"><block wx:if="{{row.status*1===1}}"><block><view data-event-opts="{{[['tap',[['cancel',['$0'],[[['t','',index,'order_no']]]]]]]}}" class="default" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['godetail',['$0'],[[['t','',index]]]]]]]}}" class="pay" bindtap="__e">付款</view></block></block><block wx:if="{{row.status*1===2}}"><block><view data-event-opts="{{[['tap',[['chakanwuliu',['$0'],[[['t','',index,'business_order_no']]]]]]]}}" class="default" bindtap="__e">物流查询</view><view data-event-opts="{{[['tap',[['cuifa',['$0'],[[['t','',index,'business_order_no']]]]]]]}}" class="pay" bindtap="__e">催发货</view></block></block><block wx:if="{{row.status*1===3}}"><block><view data-event-opts="{{[['tap',[['chakanwuliu',['$0'],[[['t','',index,'business_order_no']]]]]]]}}" class="default" bindtap="__e">物流查询</view><view data-event-opts="{{[['tap',[['shouhuo',['$0'],[[['t','',index,'business_order_no']]]]]]]}}" class="pay" bindtap="__e">确定</view></block></block><block wx:if="{{row.status*1===4}}"><block><view data-event-opts="{{[['tap',[['chakanwuliu',['$0'],[[['t','',index,'business_order_no']]]]]]]}}" class="default" bindtap="__e">物流查询</view><view data-event-opts="{{[['tap',[['gopingjia',['$0'],[[['t','',index,'business_order_no']]]]]]]}}" class="pay" bindtap="__e">去评价</view></block></block><block wx:if="{{row.status*1===6||row.status*1===7}}"><block><view data-event-opts="{{[['tap',[['seeDetil',['$0'],[[['t','',index]]]]]]]}}" class="pay" bindtap="__e">查看详情</view></block></block><block wx:if="{{row.status*1===5}}"><block><view data-event-opts="{{[['tap',[['chakanwuliu',['$0'],[[['t','',index,'business_order_no']]]]]]]}}" class="default" bindtap="__e">物流查询</view><view data-event-opts="{{[['tap',[['again',['$0'],[[['t','',index,'business_order_no']]]]]]]}}" class="pay" bindtap="__e">再来一单</view></block></block></view></view></block></block></view></view></view>