<view style="width:100%;"><view class="content"><block wx:for="{{mySell}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><view class="sells"><image class="image_" src="{{item.image[0]}}" mode></image><view class="sell_right"><view class="titles">{{item.title}}</view><view class="cost">{{"¥"+item.price}}</view><view class="flex_row"><image class="iocn" src="../../static/mysell/icon_ddfh.png" mode></image><block wx:if="{{item.order.status*1===2}}"><view class="wait">等待买家收货中</view></block><block wx:if="{{item.order.status*1===3}}"><view class="wait">等待买家收货中</view></block><block wx:if="{{item.order.status*1===4}}"><view class="wait">买家已收货</view></block></view></view></view><view class="sell_bottom"><view data-event-opts="{{[['tap',[['relation']]]]}}" class="contact" bindtap="__e">联系买家</view><block wx:if="{{item.order.status*1===2}}"><view data-event-opts="{{[['tap',[['sell',['$0'],[[['mySell','',index,'order.business_order_no']]]]]]]}}" class="contact delivery" bindtap="__e">我要发货</view></block><block wx:if="{{item.order.status*1===3}}"><view class="contact delivery">已发货</view></block><block wx:if="{{item.order.status*1===4}}"><view class="contact delivery">已完成</view></block></view></view></block></view><uni-popup class="vue-ref" vue-id="1" show="{{kanjia}}" type="center" mask-click="{{true}}" custom="{{true}}" data-ref="kanjia" bind:__l="__l" vue-slots="{{['default']}}"><view class="kanjia"><view class="flex_row"><view class="order">物流编号</view><image class="cancel" src="../../static/mysell/icon_cha.png" mode data-event-opts="{{[['tap',[['close']]]]}}" bindtap="__e"></image></view><view class="flex_row"><image class="form" src="../../static/mysell/icon_ddbh.png" mode></image><input class="fill_in" type="number" value="{{addtext}}" placeholder="请填写物流编号" data-event-opts="{{[['input',[['__set_model',['','addtext','$event',[]]]]]]}}" bindinput="__e"/></view><view style="height:1rpx;background-color:#C4C4C4;"></view><view data-event-opts="{{[['tap',[['center']]]]}}" class="confirm" bindtap="__e">确定</view></view></uni-popup></view>